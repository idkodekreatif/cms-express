                   <span class="text-xs font-weight-bolder">
                     <% const publishedDate = new Date(article.published); %>
                     <% const month = publishedDate.getMonth() + 1; %>
                     <% const day = publishedDate.getDate(); %>
                     <% const year = publishedDate.getFullYear(); %>
